{
	"name": "base",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite --host",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"migrate": "npx drizzle-kit generate:pg --config ./drizzle.config.ts",
		"push-migrations": "npx drizzle-kit push:pg --config ./drizzle.config.ts",
		"dev-express": "HOST=127.0.0.1 PORT=5173  ORIGIN=http://localhost:5173 node express"
	},
	"dependencies": {
		"@getbrevo/brevo": "2.0.0-beta.4",
		"@lucia-auth/adapter-drizzle": "^1.0.4",
		"arctic": "^1.2.1",
		"better-sqlite3": "^9.4.3",
		"drizzle-orm": "^0.30.2",
		"express": "^4.18.3",
		"jsonwebtoken": "^9.0.2",
		"lucia": "^3.1.1",
		"nodemailer": "^6.9.13",
		"oslo": "^1.1.3",
		"otp-generator": "^4.0.1",
		"pg": "^8.11.4",
		"socket.io": "^4.7.5",
		"socket.io-client": "^4.7.5",
		"svelte-email": "^0.0.4",
		"svelte-sonner": "^0.3.19",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "^5.0.1",
		"@sveltejs/enhanced-img": "^0.1.9",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^8.56.0",
		"@types/express": "^4.17.21",
		"@types/jsonwebtoken": "^9.0.6",
		"@types/node": "^20.11.28",
		"@types/nodemailer": "^6.4.14",
		"@types/otp-generator": "^4.0.2",
		"@types/pg": "^8.11.4",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"drizzle-kit": "^0.20.14",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module"
}
